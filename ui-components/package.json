{
  "name": "ui-components",
  "version": "1.36.0",
  "license": "ISC",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "lib/src/index.d.ts",
  "scripts": {
    "prepare:disabled": "yarn build",
    "start:rollup": "yarn build --watch",
    "build:rollup": "rollup --config",
    "build:rollup:debug": "nodemon --exec cross-env DEBUG=true yarn build:rollup",
    "build:rollup:watch": "yarn build:rollup --watch",
    "build:rollup:analyze": "cross-env ANALYZE_BUNDLE=true yarn build:rollup",
    "postbuild:rollup": "yarn build:declaration",
    "build:declaration": "tsc --emitDeclarationOnly",
    "start": "yarn storybook",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "build:watch": "cross-env NODE_ENV=development webpack --mode=development --watch",
    "postbuild": "yarn agadoo",
    "check-treeshake": "agadoo ./dist/esm/index.js",
    "check-treeshake:dump": "yarn check-treeshake > treeshake.dump",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:deploy": "storybook-to-aws-s3 --bucket-path=openweb-storybook/ui-components --s3-sync-options=--acl=public-read"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "resolutions": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.5",
    "@babel/plugin-external-helpers": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.10.5",
    "@rollup/plugin-babel": "^5.1.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@rollup/plugin-url": "^5.0.1",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-info": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/addon-notes": "^5.3.18",
